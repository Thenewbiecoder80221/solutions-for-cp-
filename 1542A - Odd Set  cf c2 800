#include<bits/stdc++.h>
using namespace std;
int main(){
    int t;
    cin>>t;
    while(t--){
        int n;
        cin>>n;
        int arr[200];
        vector<int> odd(2*n);
        vector<int> even(2*n);
        for(int i=0;i<2*n;i++){
            cin>>arr[i];
            if(arr[i]%2==0){
                even.push_back(arr[i]);
                
            }
            else{
                odd.push_back(arr[i]);
            }

        }
        if(odd.size()==even.size()){
            cout<<"Yes"<<endl;
        }
        else{
            cout<<"No"<<endl;
        }




    }
}
