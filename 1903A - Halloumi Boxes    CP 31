#include<bits/stdc++.h>
using namespace std;
int main(){
    int t;
    cin>>t;
    
    
 for(int j=0;j<t;j++){
    int n,k;
    cin>>n>>k;
    vector<int>a(n);
    for(int i=0;i<n;i++){
        cin>>a[i];
    }
    if(k==1){
        if(is_sorted(a.begin(),a.end())){
            cout<<"YES"<<endl;
                    }
        else{
            cout<<"NO"<<endl;
        }

    }
    if(k>1){
        cout<<"YES"<<endl;
    }
 }
